{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@user/(.*)$": "<rootDir>/../src/user/$1",
    "^@favourite/(.*)$": "<rootDir>/../favourite/$1",
    "^@review/(.*)$": "<rootDir>/../review/$1"
  }
}
